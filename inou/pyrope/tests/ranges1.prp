// code/ranges1.prp
I((1,2,3) == 1..3)
I((1,2,3) == (1..3))
I(((0..7) ..by.. 2) == (0, 2, 4, 6))
I((1..2) ..or.. 3 == (1..3))
I((1..10) ..and.. (2..20) == (2..10))

// ranges can be open
I((3..) ..and.. (1..5) == (3..5))
I((..)  ..and.. (1..2) == (1..2))
I((..4) ..or..     (2..3) == (..4))
I((..)   ..and.. (2..7) == (2..7))
I((0..)  ..and.. (2..7) == (2..7))
I((..-1) ..and.. (2..7) == (2..7))
I((..-2) ..and.. (2..7) == (2..6))
I((3..-2) ..and.. (2..7) == (3..6))

// closed ranges are like sets
I((1..3)[[]] == (1,2,3)[[]])
I(0b0010101.__set == (0,2,4))
I(0b0011110.__set == (1..4))
