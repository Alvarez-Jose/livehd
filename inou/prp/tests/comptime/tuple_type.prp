/*
:name: tuple_type
:type: parsing
*/

cassert((a=1,b=2) has "a")

cassert (a=1,b=3) does (b=100,a=333,e=40,5)
cassert (a=1,3) does (a=100,300,b=333,e=40,5)
cassert (a=1,3) !does (b=100,300,a=333,e=40,5)

cassert (x=1,d=4) equals (d=4,x=100)

// Some unnamed fields, position should also match
cassert (x=1,4) equals (d=4,100)
cassert (x=1,4) !equals (100,d=4)

cassert (x=1,d=4) == (d=4,x=1)

// Some fields unaned, position should also match
cassert (x=1,4) == (x=1,4)
cassert (x=1,4) != (4,x=1)
