
a = 3
cassert a.::[size] == 1

b = (1,2,3)
cassert b.::[size] == 3

c = 0
cassert c.::[size] == 1

d = true
cassert d.::[size] == 1

e = ("hello", "world")
cassert e.::[size] == 2

f = string("hello", "world")
cassert f.::[size] == 1

g = 44 ++ 33
cassert g.::[size] == 2

h = 1..=3
cassert h.::[size] == 3

let j = fun() { ret (1,2) }
cassert j.::[size] == 1

